import java.util.*;
public class Vowel {
    public static void main(String[] args) {
        char ch;
        System.out.print("Enter any Charecter : ");
        Scanner sc = new Scanner(System.in);
        ch = sc.next().charAt(0);
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            System.out.println("It is Vowel");
        }
        else{
            System.out.println("It is Consonent");
        }
    }
}

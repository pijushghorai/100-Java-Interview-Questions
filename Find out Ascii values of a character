import java.util.Scanner;

public class Ascii {
    public static void main(String[] args) {
        char ch;
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter charecter of Ascii values : ");
        ch = sc.next().charAt(0);
        int a = ch;
        System.out.println("The charecter of ASCII vale = "+a);
    }
}
